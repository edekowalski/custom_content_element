# Custom CE image_grid

mod.wizards.newContentElement.wizardItems.common {
	elements {
		slider {
			iconIdentifier = content-carousel-image
			title = Slider
			description = Image Slider
			tt_content_defValues {
				CType = slider
			}
		}
	}
	show := addToList(slider)
}


